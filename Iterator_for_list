import java.util.*;
public class Main {
	public static void main(String[] args) {
		
		ArrayList<Integer> arr = new ArrayList<Integer>();
		
		arr.add(67);
		arr.add(54);
		arr.add(90);
		arr.add(21);
		arr.add(43);
		
		System.out.println(arr);

    //iterator
		Iterator<Integer> i = arr.iterator();
		
		while(i.hasNext()){
			System.out.println(i.next());
		}
    //remove method 
		i.remove();
		System.out.println();
		
		System.out.println(arr);<
	
	}
}
